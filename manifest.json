{
    "name": "Note Taking Web Summarizer",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A tool for taking effiicent notes from web pages",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage",
        "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
